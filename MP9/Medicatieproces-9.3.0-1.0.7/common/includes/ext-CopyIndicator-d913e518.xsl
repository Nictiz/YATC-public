   <xsl:template name="ext-CopyIndicator"
                 match="*"
                 as="element()?"
                 mode="ext-CopyIndicator">
      <!-- Template for shared extension http://nictiz.nl/fhir/StructureDefinition/ext-CopyIndicator -->
      <xsl:param name="in"
                 as="element()?"
                 select=".">
         <!-- Optional. Ada element containing the copy indicator. Defaults to context. -->
      </xsl:param>
      <extension url="{$urlExtCopyIndicator}">
         <valueBoolean>
            <xsl:call-template name="boolean-to-boolean"/>
         </valueBoolean>
      </extension>
   </xsl:template>
</xsl:stylesheet>